n=
combinations=[]
def permutate(lst): #lst as the list that will hold combos
        if len(lst) == n:
            combo = ' '.join(str(i) for i in lst)
            if combo not in combinations:
                combinations.append(combo)
            return
        
        for i in range(1, n+1):
            if i not in lst:
                permutate(lst + [i]) #using recurrence

#using intertools

from intertools import permutations

n=
all_combs = list(permutations(range(1,n+1)))

print(len(all_combs))

for combo in all_combs:
    print(' '.join(map(str,combo)))
    
